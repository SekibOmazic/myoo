{
  "name": "myoo",
  "version": "0.0.1",
  "description": "A simple implementation of functional reactive stream library for JavaScript",
  "main": "Myoo.js",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "typings install",
    "build": "npm-run-all clean_dist copy_src compile_dist",
    "clean_dist": "rm -rf ./dist",
    "copy_src": "mkdirp ./dist/src && cp -r ./src/* ./dist/src",
    "compile_dist": "tsc ./dist/src/Myoo.ts -m commonjs --sourceMap --outDir ./dist --target ES5 -d --diagnostics --pretty --noImplicitAny --suppressImplicitAnyIndexErrors --moduleResolution node",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "mocha": "mocha tests/*.ts tests/**/*.ts --require ts-node/register",
    "postmocha": "rm -rf tests/**/*.js",
    "test": "npm-run-all lint build mocha",
    "start": "npm install && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SekibOmazic/myoo.git"
  },
  "author": "Sekib Omazic <sekib.omazic@gmail.com> (http://www.comsysto.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SekibOmazic/myoo/issues"
  },
  "homepage": "https://github.com/SekibOmazic/myoo#readme",
  "dependencies": {},
  "devDependencies": {
    "assert": "^1.4.0",
    "es6-promise": "^3.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "strip-comments": "^0.4.4",
    "npm-run-all": "^2.1.1",
    "ts-node": "^0.7.3",
    "tslint": "^3.10.2",
    "typescript": "^1.8.9",
    "typings": "^1.0.4",
    "uglify-js": "^2.6.2",
    "validate-commit-msg": "^2.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
